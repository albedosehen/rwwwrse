# Example .env file for testing Doppler integration
# Copy to .env and add your actual token

# Doppler authentication
DOPPLER_TOKEN=dp.st.your-token-here
DOPPLER_PROJECT=rwwwrse
DOPPLER_CONFIG=dev

# Basic rwwwrse configuration (will be overridden by Doppler)
RWWWRSE_LOG_LEVEL=info
RWWWRSE_PORT=8080

# Do not add sensitive values here - put them in Doppler instead